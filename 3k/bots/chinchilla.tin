#class {bot} {open}

#var {no_loop} {0}
#var {no_home} {1}
#var {hardmode} {0}

#var {botpath} {out;w;w;w;w;w;s;e;e;e;e;s;w;w;w;w;s;e;e;e;e;s;w;w;w;w;s;e;e;e;e;e;n;n;n;n;n;enter}

#act {=M= {A|An} {Baby|Young|Mature|Old} Chinchilla} {
